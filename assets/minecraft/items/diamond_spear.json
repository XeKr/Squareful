		"type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:sharpness"}],
			"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_spear/lo_sh"},
