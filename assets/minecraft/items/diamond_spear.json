		"type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:smite"}],
			"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_spear/lo_sm"},
			"on_false": {		
		"type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:bane_of_arthropods"}],
			"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_spear/lo_ba"},
			"on_false": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_spear/looting"}
			}
			}
		},

	"on_false": {

    "type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:sharpness"}],
		"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_spear/sharpness"},

	"on_false": {

    "type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:smite"}],
		"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_spear/smite"},

	"on_false": {
	
    "type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:bane_of_arthropods"}],
		"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_spear/bane_of_arthropods"},
				"on_false": {"type": "minecraft:model","model": "minecraft:item/diamond_spear"}
			}
	}
	}
	}
	},
        "when": [
          "gui",
          "ground",
          "fixed",
          "on_shelf"
        ]
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/diamond_spear_in_hand"
