		"type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:smite"}],
			"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_axe/si_sm"},
			"on_false": {		
		"type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:bane_of_arthropods"}],
			"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_axe/si_ba"},
			"on_false": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_axe/silk_touch"}
			}
			}
		},

	"on_false": {

    "type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:fortune"}],
		"on_true": {
		"type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:sharpness"}],
			"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_axe/fo_sh"},
			"on_false": {		
		"type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:smite"}],
			"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_axe/fo_sm"},
			"on_false": {		
		"type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:bane_of_arthropods"}],
			"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_axe/fo_ba"},
			"on_false": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_axe/fortune"}
			}
			}
		},

	"on_false": {

    "type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:sharpness"}],
		"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_axe/sharpness"},

	"on_false": {

    "type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:smite"}],
		"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_axe/smite"},

	"on_false": {
	
    "type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:bane_of_arthropods"}],
		"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/diamond_axe/bane_of_arthropods"},
				"on_false": {"type": "minecraft:model","model": "minecraft:item/diamond_axe"}
			}
	}
	}
	}
	}
	}
