    "cases": [
      {"when": "diamond spear prime","model": {"type": "minecraft:model","model": "minecraft:item/diamond_spear"}},
      {"when": "原初钻石矛","model": {"type": "minecraft:model","model": "minecraft:item/diamond_spear"}}
    ],
    "fallback": {

    "type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:looting"}],
