{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:component",
    "component": "minecraft:custom_name",
    "cases": [
      {"when": "netherite shovel prime","model": {"type": "minecraft:model","model": "minecraft:item/netherite_shovel"}},
      {"when": "原初下界合金锹","model": {"type": "minecraft:model","model": "minecraft:item/netherite_shovel"}}
    ],
    "fallback": {

    "type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:silk_touch"}],
		"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/netherite_shovel/silk_touch"},

	"on_false": {

    "type": "minecraft:condition","property": "minecraft:component","predicate": "enchantments","value": [{"enchantments": "minecraft:fortune"}],
		"on_true": {"type": "minecraft:model","model": "minecraft:item/basic_tools/netherite_shovel/fortune"},

	"on_false": {"type": "minecraft:model","model": "minecraft:item/netherite_shovel"}
	}
	}
	}
